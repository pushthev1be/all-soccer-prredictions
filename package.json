{
  "name": "all-soccer-predictions",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "worker": "tsx src/workers/feedback.worker.ts",
    "dev:worker": "npm run worker",
    "tokens:build": "tsx scripts/tokens/import-tokens.ts",
    "dev:all": "concurrently -n web,worker -c blue,green \"npm run dev\" \"npm run worker\"",
    "dev:sync": "set DEV_ANALYZE_SYNC=1&& next dev",
    "dev:sync:all": "set DEV_ANALYZE_SYNC=1&& concurrently -n web,worker -c blue,green \"npm run dev\" \"npm run worker\"",
    "dev:redis": "docker compose up -d redis",
    "dev:db": "docker compose up -d postgres",
    "dev:up": "docker compose up -d",
    "dev:down": "docker compose down",
    "queue:stats": "tsx scripts/queue/stats.ts",
    "db:seed-dev": "tsx scripts/seed-dev-data.ts",
    "dev:reset": "npm run dev:db && npm run db:seed-dev && npm run dev",
    "verify:real-data": "tsx scripts/verify-real-data.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^5.22.0",
    "axios": "^1.13.2",
    "bullmq": "^5.66.4",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "openai": "^6.15.0",
    "prisma": "^5.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/cheerio": "^0.22.35",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^20.19.27",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "overrides": {
    "cookie": "^0.7.0"
  }
}
